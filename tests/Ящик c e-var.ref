ноль "0";
один "1";
_2-e { ноль; один }
_8-e { _2-e; "2"; "3"; "4"; "5"; "6"; "7"; }
_10e { _8-e; "8"; "9" }
_16e { _10e; "A"; "B"; "C"; "D"; "E"; "F"; }

тип {
  _2-e . .. = . - .. " - " _2-e;
  _8-e . .. = . - .. " - " _8-e;
  _10e . .. = . - .. " - " _10e;
  _16e . .. = . - .. " - " _16e;
  .    = -;
}

очередь {
      = ;
  ? . = (? " - " <тип ? .>) "\t" <очередь .>;
}

начало = <очередь "0123456789ABCDEFG">;
