ширина; высота;

прямоугольник {
 .1 (ширина ?ш) .2 : .3 (высота ?в) .4 = (высота ": " ?в ", " ширина ": " ?ш);
 .1 (ширина ?ш) .2                     = (высота ": " ?ш ", " ширина ": " ?ш);
 .3 (высота ?в) .4                     = (высота ": " ?в ", " ширина ": " ?в);
}

начало =
   <прямоугольник (ширина 2) (высота 3)> "\n"
   <прямоугольник (высота 2) (ширина 3)> "\n"
   <прямоугольник (ширина 1)> "\n"
   <прямоугольник (высота 4)> "\n"
   ;